{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "BaseDomainApiUrl": "{{BaseDomainApiUrl}}",
  "OAuthSettings": [
    {
      "ProviderType": "LineLogin",
      "ClientId": "{{Client}}",
      "RedirectUri": "{{RedirectUri}}"
    },
    {
      "ProviderType": "LineNotify",
      "ClientId": "{{Client}}",
      "RedirectUri": "{{RedirectUri}}"
    },
    {
      "ProviderType": "FacebookLogin",
      "ClientId": "{{Client}}",
      "RedirectUri": "{{RedirectUri}}"
    },
    {
      "ProviderType": "GoogleLogin",
      "ClientId": "{{Client}}",
      "RedirectUri": "{{RedirectUri}}"
    }
  ]
}
